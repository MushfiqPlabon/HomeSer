{
  "version": 2,
  "build": {
    "env": {
      "PYTHON_VERSION": "3.10.x",
      "NODE_VERSION": "18.x"
    }
  },
  "builds": [
    {
      "src": "build.sh",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "staticfiles"
      }
    },
    {
      "src": "HomeSer/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.10",
        "includeFiles": [
          "HomeSer/**/*",
          "static/**/*",
          "templates/**/*"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "HomeSer/wsgi.py"
    }
  ],
  "env": {
    "SERVERLESS": "1"
  }
}