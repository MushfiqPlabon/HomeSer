{
    "builds": [{
      "src": "HomeSer/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    }],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "HomeSer/wsgi.py"
      }
    ],
    "buildCommand": "bash vercel-setup.sh",
    "functions": {
      "HomeSer/wsgi.py": {
        "runtime": "vercel-python@3.9.18"
      }
    },
    "github": {
      "autoJobCancelation": true
    }
}